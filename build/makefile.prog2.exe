all: prog2.exe

prog2.exe: func1.o main.o func2.2.o
	gcc -o prog2.exe func1.o main.o func2.2.o

func1.o: ../src/func1.c
	gcc ../src/func1.c -c -o func1.o

func2.2.o: ../src/finc2.2.c
	gcc ../src/finc2.2.c -c -o func2.2.o

main.o: ../src/main.c
	gcc ../src/main.c -c -o main.o

install: prog2.exe
	install ./prog2.exe /usr/local/bin

uninstall:
	rm -rf /usr/local/bin/prog2.exe

clear:
	rm -rf prog2.exe *.o

